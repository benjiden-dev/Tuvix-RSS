# ============================================================================
# Local Development Override for wrangler.toml
# ============================================================================
# Copy this file to wrangler.toml.local and fill in your actual database_id
# This file is gitignored and will override values in wrangler.toml
# ============================================================================

[[d1_databases]]
binding = "DB"
database_name = "tuvix"
database_id = "your-database-id-here"  # Replace with your actual D1 database ID

# Observability configuration for Workers logs
[observability]
[observability.logs]
enabled = false
head_sampling_rate = 1
invocation_logs = true
persist = true
